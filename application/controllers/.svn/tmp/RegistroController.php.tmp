            $size = $_FILES['qqfile']['size'];
            //echo $name; 

            if ($size == 0) {
                return array('error' => 'File is empty.');
            }

            $uploadname = $name;
            $carpeta = "uploads/";
            opendir($carpeta);
            $destino = $carpeta . $_FILES['qqfile']['name'];
            copy($_FILES['qqfile']['tmp_name'], $destino);
